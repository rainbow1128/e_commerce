{
    "pid_file" : {
        "web_api"     :"tmp/proc/order_itest_app_server.pid",
        "rpc_consumer":"tmp/proc/order_itest_rpc_consumer.pid"
    },
    "access_log": {
        "path":"tmp/log/test/order_app_server.access.log",
        "format":"%H %h %l %t \"%r\" %s %b"
    },
    "logging" : {
        "handlers" : [
            {"alias": "std-output-forall",
             "min_level": "ERROR",
             "destination": "console"}
        ],
        "loggers" : [
            {"alias": "web",
             "handlers": ["std-output-forall"] }
        ]
    },
    "listen": {
        "port": 8013,
        "host":"localhost",
        "max_failures": 5,
        "api_version": "1.0.0",
	"max_connections": 50,
        "routes": []
    },
    "limit_req_body_in_bytes": 10485760,
    "num_workers": 1,
    "stack_sz_kb": 128,
    "data_store": [
	{
	    "_type": "InMemory",
	    "alias": "keep-123-mem",
	    "max_items": 99
	}
    ],
    "rpc": {
	 "handler_type": "dummy"
    },
    "confidentiality": {
	"source": "UserSpace",
	"sys_path": "local/path/to/secret.json"
    }
}
